						"main_image": {
							"type": "Image",
							"config": {
								"label": "Main Image",
								"constraint": {},
								"thumbnails": []
							}
						},
						"main_video_url": {
							"type": "Text",
							"config": {
								"label": "Main Video Url",
								"placeholder": ""
							}
						},
						"width": {
							"type": "Number",
							"config": {
								"label": "Width",
								"placeholder": ""
