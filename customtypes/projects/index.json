			},
			"credits": {
				"type": "Group",
				"config": {
					"label": "Credits",
					"repeat": true,
					"fields": {
						"label": {
							"type": "Text",
							"config": {
								"label": "Label",
								"placeholder": ""
							}
						},
						"person": {
							"type": "Link",
							"config": {
								"label": "Person",
								"placeholder": "",
								"select": null,
								"allowTargetBlank": false,
								"allowText": false,
								"repeat": true
							}
						}
					}
				}
			},
			"slices": {
				"type": "Slices",
				"fieldset": "Slice Zone",
				"config": {
					"choices": {
