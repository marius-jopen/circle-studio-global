{
	"format": "page",
	"id": "projects",
	"label": "Projects",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"year": {
				"type": "Select",
				"config": {
					"label": "Year",
					"placeholder": "",
					"options": ["2010", "2"]
				}
			},
			"month": {
				"type": "Select",
				"config": {
					"label": "Month",
					"placeholder": "",
					"options": ["January", "2"]
				}
			},
			"date": {
				"type": "Date",
				"config": {
					"label": "Date",
					"placeholder": ""
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": ""
				}
			},
			"client": {
				"type": "Text",
				"config": {
					"label": "Client",
					"placeholder": ""
				}
			},
			"description": {
				"type": "StructuredText",
				"config": {
					"label": "Description",
					"placeholder": "",
					"allowTargetBlank": true,
					"multi": "paragraph,strong,em,hyperlink"
				}
			},
			"video_is_dark": {
				"type": "Boolean",
				"config": {
					"label": "Video starts dark",
					"placeholder_false": "false",
					"placeholder_true": "true",
					"default_value": false
