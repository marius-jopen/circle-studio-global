{
	"format": "page",
	"id": "projects",
	"label": "Projects",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"date": {
				"type": "Date",
				"config": {
					"label": "Date",
					"placeholder": ""
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": ""
				}
			},
			"client": {
				"type": "Text",
				"config": {
					"label": "Client",
					"placeholder": ""
				}
			},
			"description": {
				"type": "StructuredText",
				"config": {
					"label": "Description",
					"placeholder": "",
					"allowTargetBlank": true,
					"multi": "paragraph,strong,em,hyperlink"
				}
			},
			"preview": {
				"type": "Group",
				"config": {
					"label": "Preview",
					"repeat": true,
					"fields": {
						"preview_image_landscape": {
							"type": "Image",
							"config": {
								"label": "Preview Image Landscape",
								"constraint": {},
								"thumbnails": []
							}
						},
						"preview_video_url_landscape": {
							"type": "Text",
							"config": {
								"label": "Preview Video Url Landscape",
								"placeholder": ""
							}
						},
						"preview_image_portrait": {
							"type": "Image",
							"config": {
								"label": "Preview Image Portrait",
								"constraint": {},
								"thumbnails": []
							}
						},
						"preview_video_url_portrait": {
							"type": "Text",
							"config": {
								"label": "Preview Video Url Portrait",
								"placeholder": ""
							}
						}
					}
				}
			},
			"main": {
				"type": "Group",
				"config": {
					"label": "Main",
					"repeat": true,
					"fields": {
						"main_image": {
							"type": "Image",
							"config": {
								"label": "Main Image",
								"constraint": {},
								"thumbnails": []
							}
						},
						"main_video_url": {
							"type": "Text",
							"config": {
								"label": "Main Video Url",
								"placeholder": ""
							}
						},
						"playmode": {
							"type": "Select",
							"config": {
								"label": "Playmode",
								"placeholder": "",
								"options": ["test-1", "test-2"],
								"default_value": "test-1"
							}
						}
					}
				}
			},
			"credits": {
				"type": "Group",
				"config": {
					"label": "Credits",
					"repeat": true,
					"fields": {
						"label": {
							"type": "Text",
							"config": {
								"label": "Label",
								"placeholder": ""
							}
						},
						"person": {
							"type": "Link",
							"config": {
								"label": "Person",
								"placeholder": "",
								"select": null,
								"allowTargetBlank": false,
								"allowText": false,
								"repeat": true
							}
						}
					}
				}
			},
			"slices": {
				"type": "Slices",
				"fieldset": "Slice Zone",
				"config": {
					"choices": {
						"documentation": {
							"type": "SharedSlice"
						}
					}
				}
			}
		},
		"SEO & Metadata": {
			"meta_title": {
				"config": {
					"label": "Meta Title",
					"placeholder": "A title of the page used for social media and search engines"
				},
				"type": "Text"
			},
			"meta_description": {
				"config": {
					"label": "Meta Description",
					"placeholder": "A brief summary of the page"
				},
				"type": "Text"
			},
			"meta_image": {
				"config": {
					"constraint": {
						"height": 1260,
						"width": 2400
					},
					"label": "Meta Image",
					"thumbnails": []
				},
				"type": "Image"
			}
		}
	}
}
